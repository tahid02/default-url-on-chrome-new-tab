{
  "manifest_version": 2,
  "name": "Nick Daniele",
  "description": "quickly launch Nick Daniele website on new tab open in chrome",
  "version": "1.0.0",
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["tabs"]
}
